  /**************************FILE EXTENSION VALIDATION*******************************************/
  function checkFile(file) {
  var extension = file.substr((file.lastIndexOf('.') +1));
  if (!/(pdf|zip|doc|mp3|mp4|jpg|jpeg|png|gif|JPG)$/ig.test(extension)) {
    return false;
  }
  else
  {
    return extension;
  }
}  

    
