  function changeTab(id,elem)
  {
    
    $('.'+id+'-tab').removeClass('active');
    $('[data-tab='+$(elem).attr('id')+']').addClass('active');



    $('.'+id+'section >section').addClass('hidden');
    $('.'+id+'section > .'+id+'-'+$(elem).attr('id')).removeClass('hidden');
    $('.'+id+'section >section').removeClass('active');
    $('.'+id+'section > .'+id+'-'+$(elem).attr('id')).addClass('active');

  }

function putemoji(elem)
{
    var data=$(elem).attr('id').split('^');
   
    var msg=$('#'+data[0]+'-msg').val();
    msg+=' '+':'+data[1]+': ';

    $('#'+data[0]+'-msg').val(msg);
}

   
